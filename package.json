{
  "name": "flight-tutorials-client",
  "version": "1.0.0",
  "description": "Alces Flight Tutorials client React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "flow": "flow",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:style": "eslint --config ./.eslintrc.airbnb src",
    "lint:style:fix": "eslint --config ./.eslintrc.airbnb --fix src"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "debug": "^2.6.8",
    "flight-common": "git+https://github.com/alces-software/flight-common.git#react-15-react-router-4",
    "react-bootstrap": "^0.31.0",
    "react-fontawesome": "^1.6.1",
    "react-markdown": "^2.5.0",
    "reactstrap": "^4.8.0",
    "socket.io-client": "^2.0.3",
    "socket.io-stream": "^0.9.1",
    "styled-components": "^2.1.2",
    "terminal.js": "^1.0.9",
    "url-search-params-polyfill": "^2.0.0"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^1.0.4",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "jest-fetch-mock": "^1.1.1",
    "nwb": "0.16.x",
    "nwb-sass": "^0.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "resolve-url-loader": "^2.1.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](dist|docs|node_modules|scripts)[/\\\\]"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(scss|css$)": "<rootDir>/config/jest/cssTransform.js"
    }
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "es2016",
      "stage-2"
    ]
  }
}
